{
  "version": 2,
  "regions": ["sin1"],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "APP_ID": "${APP_ID}",
    "APP_SECRET": "${APP_SECRET}",
    "APP_TOKEN": "${APP_TOKEN}",
    "TABLE_ID": "${TABLE_ID}"
  }
}